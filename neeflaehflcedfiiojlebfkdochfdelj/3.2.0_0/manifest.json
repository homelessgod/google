{
   "background": {
      "scripts": [ "js/background.min.js" ]
   },
   "browser_action": {
      "default_icon": "icon_idle.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "js/webResource.min.js" ],
      "matches": [ "https://*/*", "http://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "The Detectify Chrome extension allows you to generate a trail to help our scanner crawl your website.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Detectify Scan Behind Login",
   "permissions": [ "\u003Call_urls>", "tabs", "webRequest", "webRequestBlocking", "webNavigation" ],
   "short_name": "Detectify",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.2.0"
}
