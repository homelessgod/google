      "128": "images/stable/icon-128.png",
      "16": "images/stable/icon-16.png",
      "32": "images/stable/icon-32.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "65",
   "name": "Secure Shell Extension",
   "offline_enabled": true,
   "omnibox": {
      "keyword": "ssh"
   },
   "options_page": "html/nassh_preferences_editor.html",
   "permissions": [ "clipboardRead", "clipboardWrite", "idle", "notifications", "storage", "unlimitedStorage", "accessibilityFeatures.read" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.17",
   "web_accessible_resources": [ "html/nassh.html", "html/nassh_google_relay.html", "html/nassh_connect_dialog.html" ]
}
